	"version": "0.2.0",
	"configurations": [
		{
			"name": "Start Dev Server",
			"type": "node",
			"request": "launch",
			"args": ["${workspaceFolder}/start.js"],
			"cwd": "${workspaceFolder}",
			"protocol": "inspector",
			"outputCapture": "std",
			"env": {}
		},
		{
			"name": "Start Prod Server",
			"type": "node",
			"request": "launch",
			"args": ["${workspaceFolder}/prod.js"],
			"cwd": "${workspaceFolder}",
			"protocol": "inspector",
			"outputCapture": "std",
			"env": {
				"NODE_ENV": "production"
			}
		}
	]
}